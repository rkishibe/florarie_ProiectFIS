<?import javafx.scene.text.*?>

<AnchorPane fx:id="adminHome" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #c96991;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.florarie_proiect.controllers.AdminHome">
    <children>
        <Label id="page" layoutX="218.0" layoutY="36.0" text="Hello Admin" textFill="#f8f4f4">
            <font>
                <Font name="Bodoni MT Bold Italic" size="28.0" />
            </font>
        </Label>
        <Button fx:id="removeButton" layoutX="200.0" layoutY="165.0" mnemonicParsing="false" onAction="#switchToSceneRemove" prefHeight="47.0" prefWidth="202.0" style="-fx-background-color: PINK;" text="REMOVE">
         <font>
            <Font name="Bodoni MT" size="14.0" />
         </font></Button>
        <Button fx:id="homeButton" layoutX="199.0" layoutY="308.0" mnemonicParsing="false" onAction="#switchToSceneHome" prefHeight="47.0" prefWidth="202.0" style="-fx-background-color: PINK;" text="HOME LOGIN/REGISTER">
