            <Font name="Bodoni MT" size="14.0" />
         </font></Button>
        <Button fx:id="addButton" layoutX="203.0" layoutY="96.0" mnemonicParsing="false" onAction="#switchToSceneAdd" prefHeight="47.0" prefWidth="196.0" style="-fx-background-color: PINK;" text="ADD">
